{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Posts"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "body": {
      "type": "dynamiczone",
      "components": [
        "blog.you-tube-embed",
        "blog.text-block",
        "blog.single-image",
        "blog.multiple-images"
      ],
      "required": true,
      "min": 1
    }
  }
}
